<h1>Season <%= @bach.season_number %> Contestants</h1>

<% @bach.contestants.each do |contestant| %>
  <div id="contestant-<%= contestant.id %>">
    <h3><%= link_to contestant.name, contestant_path(contestant) %><h3>
    <p>Age: <%= contestant.age %></p>
    <p>Hometown: <%= contestant.hometown %></p>
  </div>
<% end %>

<h3>These Contestants are from these hometowns: <%= @bach.contestant_city_list %><h3>